<div class="container-fluid">
    <div class="row profile">
        <!-- PAGE CONTENT -->
        <div class="col-md-10">
            <div class="profile-content">

            
			<div class="well">

				<h1>Edit Event</h1>

				<%= form_for @event, :method => :put do |f| %>
					<%= render partial: "form", :locals => {:f => f} %>

				  <p>
				    <%= f.submit "Update Event", :class => 'btn btn-primary btn-lg' %>
				    <%= link_to "Cancel", event_path, :class => 'btn btn-primary btn-lg' %>
				  </p>
				    
					
				<% end %>

			

              

            

            </div>
        </div>



    </div>
</div>


